   "cell_type": "code",
   "execution_count": null,
   "metadata": {},
   "outputs": [],
   "source": [
    "pipelines_dl3 = {}\n",
    "\n",
    "pipelines_dl3[\"SpatialSincBBlNet\"] = make_pipeline(create_clf(\n",
    "    model=SpatialSincBBlNet(\n",
    "        n_chans=22,\n",
    "        n_outputs=3,\n",
